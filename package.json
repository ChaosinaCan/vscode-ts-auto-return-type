{
	"name": "vscode-ts-auto-return-type",
	"displayName": "TsAutoReturnType",
	"description":
		"Automatically print inferred return on your function.",
	"version": "0.0.1",
	"publisher": "ebrithil-123",
	"engines": {
		"vscode": "^1.19.0"
	},
	"categories": ["Other"],
	"activationEvents": [
		"onCommand:extension.fixPointedFunctions",
		"onCommand:extension.fixAllFunctionsInFile"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.fixPointedFunctions",
				"title": "TsAutoReturnType: Selected function"
			},
			{
				"command": "extension.fixAllFunctionsInFile",
				"title": "TsAutoReturnType: File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	}
}
